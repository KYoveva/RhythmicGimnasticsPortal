@model IEnumerable<RhythmicGymnasticsPortal.Web.Models.NewsModels.NewsSimpleViewModel>

@{
    ViewBag.Title = "_SimpleNewsPartial";
}

@foreach (var news in Model)
{
    <div class="list-group">
        <a href="#" class="list-group-item active">
            <h4 class="list-group-item-heading">@news.Title</h4>
            <p class="list-group-item-text">@news.Content</p>
        </a>
    </div>
}

