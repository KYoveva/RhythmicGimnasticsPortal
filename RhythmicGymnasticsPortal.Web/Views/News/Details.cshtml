@model RhythmicGymnasticsPortal.Web.Models.NewsModels.NewsDetailsViewModel

@{
    ViewBag.Title = "Details";
}
<div class="container">
    <div class="row">
        <div class="col-md-8 col-md-offset-2">
            <br>
            <div class="row">
                <div class="col-sm-8">
                    <p class="text-muted">@Html.DisplayFor(m => m.Title, "Content")</p>
                    <p>
                        @Html.DisplayFor(m => m.Content, "Content");
                    </p>
                    <p class="text-muted"><span class="glyphicon glyphicon-calendar"></span>&nbsp;@Html.DisplayFor(m => m.DateCreated)  <span class="badge">by: @Html.DisplayFor(m => m.Author) </span></p>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryajax")
    @Scripts.Render("~/bundles/backbuttonlogic")
}
